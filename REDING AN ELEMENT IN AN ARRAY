//READING THE ELEMENT IN THE ARRAY TIME COMPLEXITY IS O(N)
#include<stdio.h>
int main()
{
    int arr[]={2,3,5,7,9,0};
    int n=sizeof(arr)/sizeof(arr[0]);
    int index=4;
    if(index>=0&&index<n)
    {
       // printf("value  of the element at the index position %d is %d",index,arr[index]);
       printf("a[%d]=%d",index,arr[index]);
    }
}

