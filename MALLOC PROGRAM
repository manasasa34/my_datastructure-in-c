#include <stdio.h>
#include <stdlib.h>

int main() {
    int *ptr = (int*) malloc(10 * sizeof(int));  // allocate memory for 5 integers

    if (ptr == NULL) {
        printf("Memory not allocated\n");
        return 0;
    }

    for (int i = 0; i < 10; i++)
        scanf("%d",&ptr[i]);

    for (int i = 0; i < 10; i++)
        printf("\n\n%d\n", ptr[i]);
        

    free(ptr);
    return 0;
}
