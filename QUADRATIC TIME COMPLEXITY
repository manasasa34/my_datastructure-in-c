#include <stdio.h>
#include <time.h>

int main() {
    long long n;
    printf("Enter n: ");
    scanf("%lld", &n);

    clock_t start = clock();

    long long count = 0;

    for (long long i = 0; i < n; i++) {
        for (long long j = 0; j < n; j++) {
            count++;  
        }
    }

    clock_t end = clock();

    double time_taken = (double)(end - start) / CLOCKS_PER_SEC;

    printf("Count = %lld\n", count);
    printf("Time Taken (O(n^2)) = %f seconds\n", time_taken);

    return 0;
}
