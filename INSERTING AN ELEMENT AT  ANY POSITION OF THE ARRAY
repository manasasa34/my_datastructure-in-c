//ARRAY OPERATIONS
//INSERT AN ELEMENT AT THE BEGINING OF THE ARRAY
#include<stdio.h>
int main()
{
    int arr[]={1,4,3,7,6};
    int n=sizeof(arr)/sizeof(arr[0]);
   int  position=3;
   int  value=0;
   if(position<n){
    for (int i=n;i>position;i--)
    {
        arr[i]=arr[i-1];
    }
    arr[position]=value;
    n++;
    printf("array elements : ");
    for(int i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }}
    else
    {
        printf("invalid position");
    }
    printf("\nreported by MANASA S A\n USN:1SV24CS069");
}
