#include <stdio.h>
#include <stdlib.h>
int main()
{
	int *arr,i;
	arr=(int*)calloc(5,sizeof(int));
	if(arr==NULL)
	{
		printf("memory is not allocated\n");
	return 1;
	}
	printf("by using calloc memory allocation\n");
	for(i=0;i<5;i++)
	{
		scanf("%d",&arr[i]);
	}
	for(i=0;i<5;i++)
	{
		printf("%d",arr[i]);
	}
	free(arr);
	return 0;
}
