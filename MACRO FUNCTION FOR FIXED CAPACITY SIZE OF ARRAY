//FIXED CAPACITY BY USING MACRO FUNCTION CALLED CAP
#include<stdio.h>
#define CAP 9
void display(int a[],int n);
void display(int a[CAP],int n){
printf("[");
for(int i=0;i<n&&i<CAP;i++)
{a[i]=i;
    printf("%d ",a[i]);

}printf("]");
}
int main()
{
    int a[CAP],n=100;
    display(a,n);
    return 0;
}
