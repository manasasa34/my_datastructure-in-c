
#include<stdio.h>
#define SIZE 5

int rear=-1;
int front=-1;
int queue[SIZE];


void enqueue(int value){
  
    if(rear==SIZE-1)
    {
        printf("queue is FULL\n");
        return;
    }
    else
    {if(front==-1)
        front=0;
    
    rear++;
    queue[rear]=value;
   // printf("%d inserted\n",value);
    }
}
void dequeue(){
    if(front==-1||front>rear)
    {
        printf("queue is empty there is nothing to delete\n");
        return;
    }
    
    
        printf("%d deleted\n",queue[front]);
        front ++;
        if (front > rear)
        front = rear = -1;
    // for(int i=front;i<rear;i++){
         //   queue[i-1]=queue[i];
        //rear--;
//    }
}
 void display(){
    // int queue[SIZE];
    if(front==-1||front>rear)
  {  printf("queue is empty\n");
    return;
  }
    
        printf("queue elements:");
        for(int i=front;i<=rear;i++)
        {
            printf("%d ",queue[i]);
        }
        printf("\n");
    
}
void peek()
{
    if(front==-1||front>rear)
    printf("//in peek operation queue is empty\n");
    else
    printf(" // in peek operation front element=%d\n",queue[front]);
}
int main()
{  
    enqueue(20);
    enqueue(10);
    enqueue(30);
    enqueue(40);
    enqueue(50);
    
    display();
    
    peek();
    
    dequeue();
    dequeue();
    dequeue();
    dequeue();
    dequeue();
    
    display();
    peek();
     enqueue(5);
    enqueue(15);
    display();
     dequeue();
   //  dequeue();
    
    display();
    return 0;
}
