#include <stdio.h>
#include <time.h>

int main() {
    long long n;
    printf("Enter n: ");
    scanf("%lld", &n);

    clock_t start = clock();

    long long sum = 0;
    for (long long i = 0; i < n; i++) {
        sum += i;  
    }

    clock_t end = clock();

    double time_taken = (double)(end - start) / CLOCKS_PER_SEC;

    printf("Sum = %lld\n", sum);
    printf("Time Taken (O(n)) = %f seconds\n", time_taken);

    return 0;
}
